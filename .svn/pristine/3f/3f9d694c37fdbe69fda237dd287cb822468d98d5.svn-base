/* 通用头部 */
.page-header {
  width: 100%;
  background-color: @gray-charcoal;
  position: fixed; z-index: 13; left: 0; top: 0;
  h1 {
    height: 44px;
    line-height: 44px;
    width: 60%;
    margin: 0 auto;
    text-align: center;
    font-size: 1.7rem;
    color: @white;
    font-weight: bold;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    display: -webkit-box;
    overflow: hidden;
  }
  a.ph-btn{
    display: block;
    text-decoration: none;
    font-size: 1.6rem;
    color: @white;
    line-height: 44px;
    padding: 0 8px
  }
  i.icon {
    padding: 0 10px;
    font-style: normal;
  }
  .left-arrow {
    //background-image: url(../static/img/sprites_icon.png);
    //background-size: 480%;
    //background-repeat: no-repeat;
    background-position: -94px 0px;  height: 22px; overflow: hidden; position: relative; top: .4rem;
  }
  i.down-arrow, i.down-arrow-top{
    //background-image: url(../static/img/sprites_icon.png);
    //background-size: 800%;
    //background-repeat: no-repeat;
    //background-position: -140px -26px;
    transition: all .3s ease-in-out;
    -webkit-transition: all .3s ease-in-out;
    position: relative;
    top: -3px; left: .3rem;
    border: 0.5rem solid transparent;
    font-size: 0px;
    padding: 0;
    width: 0px;
    height: 0px;
    border-top-color: #fff;
  }
  i.down-arrow-top{
    border: 0.5rem solid transparent;
    border-bottom-color: #fff;
    top: -10px;
  }
  i.right-plus { background-position: -61px -62px; width: 23px; height: 22px; padding: 0; margin: 1rem .6rem 0 0; }
  .menu-right {
    color: @white; font-size: 1.6rem; text-align: center;  position: absolute; right: .6rem; top: 5.2rem; width: 10rem; background-color: @gray-charcoal;
    &:after {
      border: 0.6rem solid transparent; position: absolute; top: -1.1rem; right: 1.2rem;   content: ''; font-size: 0px;padding: 0; width: 0px;  height: 0px; border-bottom-color: @gray-charcoal;
    }
    li {
      border-bottom: 1px solid #393940; padding: .9rem 0; color: #F0F0F0; margin: 0 .7rem; text-align: left; text-indent: 1.5rem;
      &:active { color: @white; background-color: #323238; }
      .sprite-scan, .sprite-plus { background-position: -86px -61px; width: 16px; height: 16px; position: relative; top: .2rem; left: -.5rem;  }
      .sprite-scan { background-position: -109px -62px; }
    }
    li:first-child { display: none; }
  }

  .right-circle {
    display: inline-block;
    padding: 0;
    border-radius: 50%;
    width: .4rem;
    height: .4rem;
    line-height: 1.7rem;
    vertical-align: middle;
    border: .2rem;
    background-color: @white;
  }

}

.content {
  position: absolute; top: 44px;  left: 0; right: 0;  .ios-scroll();   font-size: 1.4rem;
  //bottom: 50px; overflow: auto;
}

/* 通用底部导航 */
.page-navigator {

  .wrap { outline: none; }
  position: fixed;
  z-index: 13;
  bottom: 0;
  height: 50px;
  width: 100%;
  background-color: @white;
  .wrap > ul {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
  }
  li.pn-item {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    text-align: center;
    position: relative;
  }
  a {
    display: block;
    color: @gray-silver;
  }

  // 运营icon
  i.pn-icon {
    display: block;
    padding-top: 4px;
    height: 26px;
    &:before {
      content: '';
      width: 27px;
      height: 26px;
      background-position: -1px 1px;
    }
  }
  .active i.pn-icon:before {
    background-position: -1px -29px;
  }
  // 任务icon
  i.pni-task:before {
    background-position: -126px 1px;
  }
  // 设备icon
  i.pni-equipment:before {
    background-position: -31px -2px;
  }
  .active i.pni-equipment:before {
    background-position: -31px -33px;
  }
  .active i.pni-task:before {
    background-position: -126px -25px;
  }
  // 日期安排 icon
  i.pni-arrange:before {
    background-position: -61px -1px;
  }
  .active i.pni-arrange:before {
    background-position: -61px -32px;
  }

  em.pn-title {
    padding-top: 2px;
    font-size: 1.1rem;
    font-style: normal;
  }

  .active em.pn-title {
    color: @green-emerald;
  }

  .equipment-em {
    top: -98px;
    left: 62%;
    margin-left: -45px;
    text-align: center;
  }
  .expend-menu {
    position: absolute;
    box-shadow: 0px 2px 2px 1px @gray-light;
    ul {
      display: block;
      a {
        color: @gray-brownish;
        padding: 4px 0;
        border-bottom: 1px @gray-light solid;
        font-size: 1.5rem;
      }
      li {
        display: block;
        padding: 3px 16px;
        background-color: @white;

      }
      li.active > a {
        color: @green-emerald;
      }
      li:last-child a{
        border-bottom: none;
      }
    }
  }
  .pn-task { position: relative;}
  .tip-red {
    border-radius: 50%;
    font-size: 1.3rem;
    margin-left: 1rem;
    display: inline-block;
    background-color: #fe0000;
    color: #fff;
    text-align: center;
    font-style: normal;
    width: .7rem;
    height: .7rem;
    line-height: .7rem;
    position: absolute;
    top: .2rem;
    left: 50%;
  }

}

.page-prompt {
  padding: 24px 40px 120px 40px;
  h3 {
    text-align: center;
    font-size: 1.6rem;
    line-height: 2.4rem;
    color: @gray-cool;
    font-weight: normal;
  }
  p {
    text-align: center;
    font-size: 1.4rem;
    line-height: 2.4rem;
    color: @gray-cool;
  }
}

/* mask */
.mask { position: fixed; left: 0; top: 44px; bottom: 0; right: 0; background-color: @gray-base; opacity: .3; z-index: 14;}

.menu {
  position: absolute;
  top: 44px;
  left: 0;
  margin-bottom: 56px;
  width: 100%;
  z-index: 10;
  ul {
    list-style: none;
    position: fixed;
    top: 44px;
    left: 0px;
    z-index: 13;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
    overflow: auto;
  }
  li {
    background-color: @white;
    font-size: 1.6rem;
    text-align: center;
    /*padding: 6px 0;*/
    height: 4.4rem;
    line-height: 4.4rem;
    border-bottom: 1px @gray-light solid;
  }
  a {
    color: @gray-base*6;
  }
}

.temp-image {
  width: 100%;
}



