.common-bg {
    padding: 0 0.84rem;
    margin-bottom: 0.42rem;
    background-color: @white;
}
.common-tip {
    display: inline-block;
    height: 3.58rem;
    line-height: 3.58rem;
    padding-left: 1.25rem;
    color: @gray-charcoal;
}
.detail {
    font-size:1.33rem;
    .timeout{
        height: 2.83rem;
        line-height: 2.83rem;
        background: #FFE8E6;
        color: @red-pale-red;
        span{
            display: inline-block;
            width: 1.25rem;
            height: 1.25rem;
            background: url('../../static/img/Group5.png');
            background-size: cover;
            margin: -0.18rem 0.5rem  0 1.34rem;
            vertical-align: middle;
        }
    }
    
    
    .detail-base {
        .detail-title{
            color:@gray-cool;
            padding: 1.17rem 1.16rem 1.25rem 0.42rem;  
            width:5.33rem;
        }
        
        .detail-cause {
            width: 100%;
            position: relative; 
            border-bottom: 1px solid  @gray-bg;
            .detail-title{ 
                top: 0;
                left: 0;
                position: absolute;
            }
            .detail-describe {
                line-height: 2rem;
                margin: 1.17rem 0.67rem 1.25rem 7.24rem;
            }
        }
    }
    .detail-service { 
        .txt {
            color:@gray-brownish;
            // height: 3.71rem;
            padding-left: 1.25rem;
            line-height: 1.8rem;
            padding: 0.83rem 1.25rem;
            border-top: 1px solid  @gray-bg;
        }
    }
    .detail-assist {
    //    height: 3.71rem;
       padding-left: 1.25rem;
       color:@gray-brownish;
       border-top:1px solid  @gray-bg;
       position: relative;
       .txt {
            color:@gray-brownish;
            // height: 3.71rem;
             line-height: 1.8rem;
            padding: 0.83rem 0;
            border-top: 1px solid  @gray-bg;
        }
       .name,.icons {
           position: absolute; 
           top:0; 
           line-height: 3.71rem;
           display: inline-block;
       }
       .name {left: 1.25rem;}
       .icons {right:0.42rem;}
       .cause {
           margin:0 2.03rem 0 4.83rem ;
           height: 100%;
           line-height: 3.71rem;
           white-space:nowrap; 
           overflow:hidden; 
           text-overflow:ellipsis;
           }
    }
    .detail-handle {
        margin-bottom:5.27rem;
        .time {
            float: right;
            margin-right: 0.42rem;
            color:@gray-brownish;
        }
        .handle-list {
            border-top:1px solid  @gray-bg;
            padding-left: 1.25rem;
            ul{
                border-left:2px solid  @gray-bg;
                li{
                    position: relative;
                    height: 5.66rem;
                    margin-left:1.5rem;
                    border-bottom:1px solid  @gray-bg;
                    &:first-child{
                        & ::after {
                            display: inline-block;
                            content: '';
                            background: @gray-cool;
                            width: 0.66rem;
                            height: 0.66rem; 
                            border-radius: 50%;
                            position: absolute;
                            top:1.3rem;
                            left:-1.9rem;
                        }
                        &::before {
                            display: inline-block;
                            content: '';
                            background:@gray-light; 
                            width: 1.18rem;
                            height: 1.18rem; 
                            position: absolute;
                            top:1.04rem;
                            left:-2.15rem;
                        }
                    }
                    .handle-log {
                        height: 3.2rem;
                        line-height: 3.2rem;
                    }
                    .handle-time {
                        color:@gray-cool;
                    }
                    &::before{
                       display: inline-block;
                       content: '';
                       background: @gray-silver;
                       width: 0.5rem;
                       height: 0.5rem; 
                       border-radius: 50%;
                       position: absolute;
                       top:1.3rem;
                       left:-1.85rem;
                    }
                    
                }
            }
        }
        .handle-more {
            height: 3.79rem;
            line-height: 3.79rem;
            text-align: center;
            color: @gray-cool;
            span{
                display: inline-block;
                width: 1.66rem;
                height: 1.66rem;
                background: url('../../static/img/nav_filtrate_pre.png') ;
                background-size: cover;
                margin-left: 0.8rem;
                vertical-align: middle;
            }
        }
    }
    

    .process-btn {
        width: 100%;
        height: 5rem;
        line-height: 5rem;
        background-color: @white;
        position: fixed;
        left: 0; bottom: 0;
        z-index: 13;
        .green-btn {
            width: 8.08rem;
            color: @white;
            height: 5rem;
            line-height: 5rem;
            text-align: center;
            font-size:1.33rem;
            background: @green-emerald;
            float:left;
        }
        .left,
        .right{
            float:left;
            width:50%;
            margin:0 0 0 -4.13rem;
            line-height: 5rem;
            height: 5rem;
            text-align: center;
            font-size:1.33rem;
        }
        .innerLeft,
        .innerRight{
            margin:0 0 0 4.13rem;
            background-color:@white;
            div {
                float:left;
                width: 50%;
                border-left: 1px solid @gray-bg;
                box-sizing:border-box;
            }
        }
        .innerRight {
            div{
                width: 100%;
            }
        }
    }
    .assign-btn {
        width: 100%;
        height: 5rem;
        line-height: 5rem;
        background-color: @white;
        position: fixed;
        left: 0; bottom: 0;
        z-index: 13; 
        .green-btn {
            color: @white;
            font-size:1.33rem;
            background: @green-emerald;
            border: 2px solid @green-emerald;
        }
        ul {
            width: 98%;
            height: 4rem;
            margin: 0.5rem auto;
            li{
                float: left;
                width: 37.5%;
                height: 3.5rem;
                line-height: 3.5rem;
                text-align: center;
                &:last-child{
                    width:25%; 
                    text-indent: 0rem;
                }
                div{
                    width: 90%;
                    margin: 0 auto;
                    text-align: center;
                    border: 2px solid @gray-bg;
                    border-radius: 3px;
                    color:  @green-emerald;
                    position: relative;
                    .return-icon{
                        background: url('../../static/img/return.png');
                        background-size: cover;
                    }
                    .pass-icon {
                        background: url('../../static/img/pass.png');
                        background-size: cover;
                    }
                    span{
                        display: inline-block;
                        width: 1.7rem;
                        height: 1.7rem;
                        line-height: 1.7rem;
                        margin-right:1rem;
                        vertical-align: middle;
                    }
                }

            }
        }
    }
}

.flow-assigner {
    height: 3.71rem;
    padding-left: 1.25rem;
    color:@gray-brownish;
    border-top:1px solid  @gray-bg;
    position: relative;
    .name,.icons {
        position: absolute; 
        top:0; 
        line-height: 3.71rem;
        display: inline-block;
    }
    .name {left: 1.25rem;color: @gray-charcoal;}
    .icons {right:0.42rem;}
    .cause {
        margin:0 2.03rem 0 4.83rem ;
        height: 100%;
        line-height: 3.71rem;
        white-space:nowrap; 
        overflow:hidden; 
        text-overflow:ellipsis;
        color: @gray-cool;
   }
}

.flow-reason {
    .fill-reason {
        border-top:1px solid  @gray-bg;
        min-height: 7.38rem;
        padding: 1.2rem 1.25rem;
        color: @gray-cool;
        textarea {
            width: 100%;
            min-height: 7.38rem;
            border: none;
        }
    }
}

.trans-accept {
    ul {
        li{
            height: 6.5rem;
            line-height: 6.5rem;
            position: relative;
            border-bottom:1px solid  @gray-bg;
            .left {
                float:left;
                width:100%;
                margin-right:-100px;
                .name{
                    width:80%;
                    height: 2rem;
                    line-height: 2rem;
                    margin: 0.83rem;
                    span{float: left}
                }
                .tag {
                    display: inline-block;
                    border:1px solid  @gray-cool;
                    height: 1.5rem;
                    line-height: 1.5rem;
                    padding: 0 0.8rem;
                    color: @gray-brownish;
                    font-size:1rem;
                    margin-left:0.5rem;
                    margin-top: 0.2rem;
                    border-radius: 0.83rem;
                }
                .info {
                    width:80%;
                    height: 2rem;
                    line-height: 2rem;
                    margin-left: 0.83rem;
                    .skill,.city{
                        width: 50%;
                        float: left;
                        color: @gray-brownish;
                        font-size:1.17rem;
                        white-space:nowrap; 
                        overflow:hidden; 
                        text-overflow:ellipsis;
                    }
                    .city {
                        text-indent: 1rem;
                    }
                    
                }
                
            }
            .right {
                position: absolute;
                right: 0;
                top:0;
                width:2.5rem;
                .active{
                    background: url('../../static/img/Group.png');
                    background-size: cover;   
                }
                span{
                    display: inline-block;
                    width: 2rem;
                    height: 2rem;
                    margin-top: 2rem;
                    background: url('../../static/img/Ring.png');
                    background-size: cover;                    
                }
            }
        }
    }
}